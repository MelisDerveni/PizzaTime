
<h1 class="mb-5">Past Orders</h1>
@foreach (var item in ViewBag.PastOrders.PizzaOrdered)
{

    <div class="row ">
    <p class="col-sm-3">Date @item.CreatedAt</p>  <p class="col-sm-2"> <input type="checkbox"> <label  for="Favorite" >Favorite</label></p>
   
</div>

<div class="row borderDown col-sm-7">
     <p class="col-sm-5">Size @item.Size - Toppings @item.Toppings</p> <p class="col-sm-2">Price $</p>
</div>
    
}

